{
    "name": "kafoso/doctrine-firebird-driver",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kasper SÃ¸fren",
            "email": "soefritz@gmail.com"
        },
        {
            "name": "Uffe Petersen"
        },
        {
            "name": "Michael Wegener",
            "email": "mw@satware.com"
        }
    ],
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "composer/package-versions-deprecated": true
        }
    },
    "autoload": {
        "psr-4": {
            "Kafoso\\DoctrineFirebirdDriver\\": "src/"
        }
    },
    "extra": {
        "bamarni-bin": {
            "bin-links": true,
            "forward-command": true
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kafoso\\DoctrineFirebirdDriver\\Test\\Resource\\": "tests/resources/Test/Resource",
            "Kafoso\\DoctrineFirebirdDriver\\Test\\Integration\\": "tests/tests/Test/Integration",
            "Kafoso\\DoctrineFirebirdDriver\\Test\\Unit\\": "tests/tests/Test/Unit",
            "Kafoso\\DoctrineFirebirdDriver\\Test\\Functional\\": "tests/tests/Test/Functional"
        }
    },
    "require": {
        "php": "^8.1",
        "doctrine/dbal": "^3.6",
        "composer-runtime-api": "^2",
        "ext-interbase": "*",
        "ext-pdo": "*"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.8",
        "dg/bypass-finals": "^1.8",
        "doctrine/orm": "^3.0",
        "fig/log-test": "^1",
        "jetbrains/phpstorm-stubs": "2023.1",
        "monolog/monolog": "^3.7",
        "phpstan/phpstan": "1.12.0",
        "phpstan/phpstan-strict-rules": "^1.6",
        "phpunit/phpunit": "9.6.20",
        "psalm/plugin-phpunit": "0.18.4",
        "slevomat/coding-standard": "8.13.1",
        "squizlabs/php_codesniffer": "3.10.2",
        "symfony/cache": "^5.4|^6.0|^7.0",
        "symfony/console": "^4.4|^5.4|^6.0|^7.0",
        "vimeo/psalm": "4.30.0"
    }
}
