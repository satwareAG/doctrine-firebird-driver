{
    "name": "satag/doctrine-firebird-driver",
    "type": "library",
    "description": "Doctrine DBAL/ORM driver for Firebird SQL 2.5 and newer",
    "license": "MIT",
    "keywords": [
        "firebird",
        "interbase",
        "fbird",
        "sql",
        "dbal",
        "orm",
        "doctrine",
        "firebird driver",
        "database driver",
        "sql driver"
    ],
    "authors": [
        {
            "name": "Michael Wegener",
            "email": "mw@satware.com"
        },
        {
            "name": "Kasper SÃ¸fren",
            "email": "soefritz@gmail.com"
        },
        {
            "name": "Uffe Petersen"
        }
    ],
    "support": {
        "issues": "https://github.com/satwareAG/doctrine-firebird-driver/issues",
        "source": "https://github.com/satwareAG/doctrine-firebird-driver"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "composer/package-versions-deprecated": true
        }
    },
    "autoload": {
        "psr-4": {
            "Satag\\DoctrineFirebirdDriver\\": "src/"
        }
    },
    "extra": {
        "bamarni-bin": {
            "bin-links": true,
            "forward-command": true
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Satag\\DoctrineFirebirdDriver\\Test\\": "tests/Test/"
        }
    },
    "require": {
        "php": "^8.1",
        "ext-interbase": "*",
        "ext-pdo": "*",
        "composer-runtime-api": "^2",
        "doctrine/dbal": "^4.1"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.8",
        "dg/bypass-finals": "^1.8",
        "doctrine/coding-standard": "12.0.0",
        "doctrine/deprecations": "^0.5.3|^1",
        "doctrine/orm": "^3.0",
        "fig/log-test": "^1",
        "jetbrains/phpstorm-stubs": "2024.2",
        "monolog/monolog": "^3.7",
        "phpstan/phpstan": "1.12.5",
        "phpstan/phpstan-doctrine": "^1.5",
        "phpstan/phpstan-strict-rules": "^1.6",
        "phpunit/phpunit": "^10.5",
        "psalm/plugin-phpunit": "0.19.0",
        "slevomat/coding-standard": "8.15.0",
        "squizlabs/php_codesniffer": "3.10.3",
        "symfony/cache": "^5.4|^6.0|^7.0",
        "symfony/console": "^4.4|^5.4|^6.0|^7.0",
        "vimeo/psalm": "^5.0"
    }
}
